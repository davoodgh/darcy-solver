FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
	volTensorFieldValue K    (1e-13 0 0 0 1e-13 0 0 0 1e-13)
	volScalarFieldValue WInj1 0
	volScalarFieldValue WPro1 0
);



regions
(
    // injection
   boxToCell
   {
     box (0 0 38) (1 1 40);
     fieldValues
	(
	   volScalarFieldValue WInj1 1 
     	);
   }   

   // production
    boxToCell
   {
     box (39 0 0) (40 1 2);
     fieldValues
        (
           volScalarFieldValue WPro1 1 
        );
   }

   // layer2
    boxToCell
   {
     box (0 0 10) (40 1 30);
     fieldValues
        (
           volTensorFieldValue K  (1e-13 0 0 0 1e-13 0 0 0 1e-13) 
        );
   }     
     
// layer3
    boxToCell
   {
     box (0 0 30) (40 1 40);
     fieldValues
        (
           volTensorFieldValue K  (1e-13 0 0 0 1e-13 0 0 0 1e-13)
        );
   }           
     
  

);



