FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
	volTensorFieldValue K    (1e-11 0 0 0 1e-09 0 0 0 1e-10)
	volScalarFieldValue WInj1 0
	volScalarFieldValue WPro1 0
);



regions
(
    // injection
   boxToCell
   {
     box (1 1 8) (2 2 20);
     fieldValues
	(
	   volScalarFieldValue WInj1 1 
     	);
   }   

   // production
    boxToCell
   {
     box (97 18 0) (98 19 12);
     fieldValues
        (
           volScalarFieldValue WPro1 1 
        );
   }

   // layer2
    boxToCell
   {
     box (0 0 5) (100 20 10);
     fieldValues
        (
           volTensorFieldValue K  (2e-11 0 0 0 3e-09 0 0 0 2e-10) 
        );
   }     
     
// layer3
    boxToCell
   {
     box (0 0 10) (100 20 20);
     fieldValues
        (
           volTensorFieldValue K  (3e-11 0 0 0 2e-09 0 0 0 3e-10)
        );
   }           
     
  

);



