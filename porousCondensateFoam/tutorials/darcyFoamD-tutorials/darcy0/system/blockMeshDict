FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 1;

xMin 0;
xMax 100;
yMin 0;
yMax 20;
zMin 0;
zMax 20;

Nx 100;
Ny 20;
Nz 20;

vertices
(
    ($xMin $yMin $zMin) // 0
    ($xMax $yMin $zMin) // 1
    ($xMax $yMax $zMin) // 2
    ($xMin $yMax $zMin) // 3
    ($xMin $yMin $zMax) // 4
    ($xMax $yMin $zMax) // 5
    ($xMax $yMax $zMax) // 6
    ($xMin $yMax $zMax) // 7
);

blocks
(
    hex (0 1 2 3 4 5 6 7) ($Nx $Ny $Nz) simpleGrading (1 1 1)
);

edges
(
);

boundary
(

 top
 {
   type patch;
   faces ((4 5 6 7));
 }

 bottume
 {
   type wall;
   faces ((0 3 2 1));
 }  
 
 walls
 {
   type wall;
   faces
        (
         (0 4 7 3)
         (1 5 6 2)
         (0 4 5 1)
         (3 7 6 2)
        );   
 }






 
);

mergePatchPairs
(
